    Dim Fileinformation As System.IO.FileInfo
    Dim Modified As DateTime
    Public Sub Main()
        If Dts.Variables.Contains("User::ModifiedDateFrmotheScript") = True Then
            Fileinformation = New System.IO.FileInfo(Dts.Variables("User::Filefullpath").Value.ToString)
            Modified = Fileinformation.LastWriteTime
            Dts.Variables.Item("User::ModifiedDateFrmotheScript").Value = Fileinformation.LastWriteTime.ToString

            Dts.TaskResult = ScriptResults.Success

            MessageBox.Show(Dts.Variables.Item("User::ModifiedDateFrmotheScript").Value.ToString)
